{
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "eqeqeq": "error",
        "no-undef": "error",
        "use-isnan": "warn"
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    }
}
